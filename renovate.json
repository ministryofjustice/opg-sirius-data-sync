{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "packageRules": [
    {
      "description": [
        "Alpine dependency - do not upgrade from 3.19 as we need to keep postgres v13 compatibility (see SP-2168)"
      ],
      "matchFiles": [
        "Dockerfile"
      ],
      "matchDepNames": [
        "alpine"
      ],
      "enabled": false
    }
  ]
}
